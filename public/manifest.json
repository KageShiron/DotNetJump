{
  "manifest_version": 2,
  "name":"DotNetJump",
  "description": "Search multiple documents for .NET at once. Jump to another .NET document.",
  "version":"0.1.2",
  "icons": { "16": "/static/img/logo16.png",
    "48": "/static/img/logo48.png",
   "128": "/static/img/logo128.png" },
  "browser_action": {
    "default_title": "DotNetJump",
    "default_popup": "popup.html",
    "default_icon": "/static/img/logo16.png"
  },
  "permissions":["activeTab","https://docs.microsoft.com/*","https://referencesource.microsoft.com/*","https://services.social.microsoft.com/*","https://source.dot.net/*"]
}