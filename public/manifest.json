{
  "manifest_version": 2,
  "name":"DotNetJump",
  "description": "Jump to another .NET document.",
  "version":"0.1",
  "browser_action": {
    "default_title": "Dot Net Jump",
    "default_popup": "popup.html"
  },
  "permissions":["activeTab","https://docs.microsoft.com/*","https://referencesource.microsoft.com/*","https://services.social.microsoft.com/*"]
}